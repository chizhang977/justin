{
    "builds": [
      {
        "src": "docs/**/*.md",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "docs/.vitepress/dist"
        }
      }
    ],
    "routes": [
      { "src": "/justin/(.*)", "dest": "/docs/.vitepress/dist/$1" },
      { "src": "/", "dest": "/docs/.vitepress/dist/index.html" }
    ]
  }
  